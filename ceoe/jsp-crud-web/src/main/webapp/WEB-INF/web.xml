<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PersonsListController</servlet-name>
  	<display-name>PersonsListController</display-name>
  	<description></description>
  	<servlet-class>com.ceoe.java.servlet.person.PersonsListController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddPersonController</servlet-name>
  	<display-name>AddPersonController</display-name>
  	<description></description>
  	<servlet-class>com.ceoe.java.servlet.person.AddPersonController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditPersonController</servlet-name>
  	<display-name>EditPersonController</display-name>
  	<description></description>
  	<servlet-class>com.ceoe.java.servlet.person.EditPersonController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeletePersonController</servlet-name>
  	<display-name>DeletePersonController</display-name>
  	<description></description>
  	<servlet-class>com.ceoe.java.servlet.person.DeletePersonController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PersonsListController</servlet-name>
  	<url-pattern>/listPersons</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddPersonController</servlet-name>
  	<url-pattern>/addPerson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditPersonController</servlet-name>
  	<url-pattern>/editPerson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeletePersonController</servlet-name>
  	<url-pattern>/deletePerson</url-pattern>
  </servlet-mapping>
  
</web-app>
