<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>CEOE Servlet Params Sample Web Application</display-name>
	<servlet>
		<servlet-name>SampleParamsServlet</servlet-name>
		<display-name>SampleParamsServlet</display-name>
		<description>Sample Params Servlet</description>
		<servlet-class>com.ceoe.java.servlet.SampleParamsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SampleParamsServlet</servlet-name>
		<url-pattern>/SampleParamsServlet</url-pattern>
	</servlet-mapping>

</web-app>
